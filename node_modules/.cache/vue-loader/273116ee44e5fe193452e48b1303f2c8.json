n                    {name: 'Upload Page', value: 2}, {name: 'Sub Domain', value: 8}, {name: 'Site URL', value: 33}, {name: 'Resource Path', value: 186}\n                ]\n            },\n        }\n    },\n    created() {\n        let id = this.$route.query.id\n        this.getDetail(id)\n    },\n   \n    methods: {\n        getDetail(id){\n            AssetDetail({id: id}).then((response) => {\n                console.log(response.data)\n                let resp = response.data\n                this.asset_info =resp.asset_info\n                this.box1 = resp.box1\n                this.box2 = this.webhandler(resp.web_list)\n                this.box3 = resp.vul_list\n            });\n        },\n        // 将web数组进行进行组合\n        webhandler(obj){\n            for(let i=0;i<obj.series.length;i++){\n                console.log(obj.series[i])\n            }\n            let box2 = {\n                title: obj.title, type: obj.type, image: obj.image,\n            }\n            return box2\n        }\n    }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.dashboard-editor-container {\n    padding-top: 8px;\n    padding-right: 20px;\n    padding-left: 20px;\n    background-color: rgb(240, 242, 245);\n    position: relative;\n\n    .github-corner {\n        position: absolute;\n        top: 0px;\n        border: 0;\n        right: 0;\n    }\n\n    .chart-wrapper {\n        padding: 0px;\n        padding-left: -20px;\n        padding-right: -20px;\n        margin-bottom: 32px;\n    }\n}\n\n@media (max-width:1024px) {\n    .chart-wrapper {\n        padding: 8px;\n    }\n}\n\n</style>\n\n "]}]}